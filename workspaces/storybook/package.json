{
    "name": "@sepraisal/storybook",
    "version": "0.0.1",
    "description": "SE Praisal - storybook of web application.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "author": "Akuukis",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/space-engineers-community/sepraisal"
    },
    "license": "GPL-3.0",
    "private": true,
    "scripts": {
        "start": "yarn build && yarn run watch",
        "build": "tsc -v && tsc -b tsconfig-src.json",
        "clean": "rm -rf coverage lib tmp tsconfig-src.tsbuildinfo",
        "lint": "tslint -p tsconfig-src.json",
        "lint:fix": "yarn run lint --fix",
        "lint:summary": "yarn lint:verbose | grep ERROR | sed -r 's/ERROR: \\((.*)\\)\\s\\/.*/\\1/' | sort | uniq -c | sort -nr",
        "lint:verbose": "yarn run lint -t verbose",
        "test": "echo 'no tests :('",
        "test:coverage": "yarn run test --coverage",
        "watch": "yarn run watch:hot",
        "watch:src": "tsc -b tsconfig-src.json --watch  # --preserveWatchOutput",
        "watch:test": "yarn run test --watch",
        "watch:hot": "start-storybook -p 3000"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.5",
        "@material-ui/styles": "^4.9.0",
        "@sepraisal/app": "^0.0.1",
        "@sepraisal/common": "^0.0.1",
        "@storybook/addon-actions": "^5.3.14",
        "@storybook/addon-knobs": "^5.3.14",
        "@storybook/addon-links": "^5.3.14",
        "@storybook/react": "^5.3.14",
        "mobx": "^5.15.4",
        "moment": "^2.24.0",
        "react": "^16.13.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@types/react": "^16.9.23",
        "babel-loader": "^8.0.6",
        "utility-types": "^3.10.0"
    }
}
