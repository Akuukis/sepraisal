{
    "name": "@sepraisal/crawler",
    "version": "0.0.1",
    "description": "SE Praisal Crawler.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "author": "Akuukis",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/space-engineers-community/sepraisal"
    },
    "license": "GPL-3.0",
    "private": true,
    "scripts": {
        "start": "yarn run build && yarn run lint && yarn run test",
        "build": "tsc -v && tsc",
        "clean": "rm -rf coverage lib tmp tsconfig.tsbuildinfo",
        "cli": "ts-node -r dotenv/config src/cli/index.ts",
        "lint": "tslint",
        "lint:fix": "yarn run lint --fix",
        "lint:summary": "yarn lint:verbose | grep ERROR | sed -r 's/ERROR: \\((.*)\\)\\s\\/.*/\\1/' | sort | uniq -c | sort -nr",
        "lint:verbose": "yarn run lint -t verbose",
        "launch": "yarn build && ts-node -r dotenv/config src/index.ts",
        "test": "echo 'no tests :('",
        "test:coverage": "yarn run test --coverage",
        "watch": "yarn run watch:src",
        "watch:src": "tsc --watch  # --preserveWatchOutput",
        "watch:test": "yarn run test --watch"
    },
    "dependencies": {
        "@sepraisal/common": "^0.0.1",
        "@sepraisal/praisal": "^0.0.1",
        "form-data": "^3.0.0",
        "moment": "^2.24.0",
        "mongodb": "^3.5.4",
        "node-fetch": "^2.6.0",
        "pad": "^3.2.0",
        "scrape-it": "^5.2.3",
        "tail": "^2.0.3",
        "utility-types": "^3.10.0",
        "worker-farm": "^1.7.0"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.16",
        "@types/mongodb": "^3.5.0",
        "@types/node": ">= 10",
        "@types/node-fetch": "^2.5.5",
        "@types/tail": "^2.0.0"
    }
}
