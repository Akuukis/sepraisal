# EditorConfig is awesome: https://EditorConfig.org
root = true


## Defaults
[**]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 4

[**.yml]
indent_size = 2




## Ignore some checked in paths.

[LICENSE]
indent_style = unset
indent_size = unset

[yarn.lock]
indent_style = unset
indent_size = unset

[**/__snapshots__/**]
indent_style = unset
indent_size = unset

[workspaces/praisal/vendor/**]
indent_style = unset
indent_size = unset

[workspaces/app/src/types/react-table.d.ts]
indent_style = unset
indent_size = unset

